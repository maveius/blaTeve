{
  "name": "maveius/liphte",
  "type": "library",
  "description": "Lightweight & minimalistic HTML Builder/Generator for PHP",
  "keywords": ["html","builder", "wrapper", "library"],
  "homepage": "https://github.com/maveius/liphte",
  "version": "1.0",
  "license": "MIT",
  "authors": [
  {
    "name": "maveius",
    "email": "maveilthain@gmail.com",
    "homepage": "http://maveius.pl",
    "role": "Developer"
  }],
  "require": {
    "php": ">=5.6.9",
    "windwalker/dom": "~2.0",
    "windwalker/html": "~2.0",
    "roave/security-advisories": "dev-master"

  },
  "require-dev": {
    "phpunit/phpunit": ">=5.7"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "maveius/liphte",
        "version": "1.0",
        "dist": {
          "url": "https://github.com/maveius/liphte",
          "type": "vcs"
        },
        "autoload": {
          "classmap": [
            "."
          ],
          "psr-0": { "": "." }
        }
      }
    }
  ],
  "autoload": {
    "psr-4": {"liphte\\": "."},
    "classmap": [ "." ]
  }
}
